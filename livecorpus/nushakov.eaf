        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a105" ANNOTATION_REF="a89">
                <ANNOTATION_VALUE>Like, for example eighty plus percent of adult population has vaccinated.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a106" ANNOTATION_REF="a90">
